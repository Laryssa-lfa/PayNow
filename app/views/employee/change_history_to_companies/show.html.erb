<div class="container">
  <br>
  <div class="form-control">
    <h3>Detalhes</h3>
    <dl class="row">
      <dt class="col-sm-3">Razão Social</dt>
      <dd class="col-sm-9"><%= @change_history_to_company.corporate_name %></dd>
      
      <dt class="col-sm-3">CNPJ</dt>
      <dd class="col-sm-9"><%= @change_history_to_company.cnpj %></dd>

      <dt class="col-sm-3">Endereço</dt>
      <dd class="col-sm-9"><%= @change_history_to_company.address %></dd>
        
      <dt class="col-sm-3">E-mail</dt>
      <dd class="col-sm-9"><%= @change_history_to_company.email %></dd>
       
      <dt class="col-sm-3">Status</dt>
      <dd class="col-sm-9"><%= @change_history_to_company.status %></dd>
        
      <dt class="col-sm-3">Token</dt>
      <dd class="col-sm-9"><%= @change_history_to_company.token %></dd>
      
      <dt class="col-sm-3">Criado em</dt>
      <dd class="col-sm-9"><%= @change_history_to_company.created_at %></dd>
      
      <dt class="col-sm-3">Alterado em</dt>
      <dd class="col-sm-9"><%= @change_history_to_company.updated_at %></dd>
    </dl>
  </div>
</div>
